{
  "$schema": "vscode://schemas/color-theme",
  "name": "OneDark Alabaster",
  "type": "dark",
  "tokenColors": [
    {
      "name": "Base text - not highlighted",
      "scope": [
        "source",
        "text",
        "variable",
        "variable.other.readwrite",
        "variable.other.object",
        "variable.other.property",
        "variable.parameter",
        "support.variable",
        "meta.object-literal.key"
      ],
      "settings": {
        "foreground": "#d5c4a1"
      }
    },
    {
      "name": "Comments - gray italic",
      "scope": [
        "comment",
        "punctuation.definition.comment"
      ],
      "settings": {
        "foreground": "#8a8a8a",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Strings",
      "scope": [
        "string",
        "string.quoted",
        "string.template",
        "string.regexp",
        "constant.other.symbol"
      ],
      "settings": {
        "foreground": "#85ad85"
      }
    },
    {
      "name": "String escape sequences",
      "scope": [
        "constant.character.escape",
        "constant.other.placeholder"
      ],
      "settings": {
        "foreground": "#949494"
      }
    },
    {
      "name": "Numbers and Constants",
      "scope": [
        "constant.numeric",
        "constant.character",
        "constant.language.boolean",
        "constant.language.null",
        "constant.language.undefined",
        "constant.language"
      ],
      "settings": {
        "foreground": "#ff8700"
      }
    },
    {
      "name": "Global definitions (functions, classes) - italic",
      "scope": [
        "entity.name.function",
        "entity.name.class",
        "entity.name.type",
        "support.class",
        "support.type"
      ],
      "settings": {
        "foreground": "#83adad",
        "fontStyle": "italic"
      }
    },
    {
      "name": "Keywords - base text color",
      "scope": [
        "keyword",
        "keyword.control",
        "keyword.operator",
        "keyword.other",
        "storage.type",
        "storage.modifier"
      ],
      "settings": {
        "foreground": "#dab997"
      }
    },
    {
      "name": "Function/method calls - base text color",
      "scope": [
        "meta.function-call",
        "support.function"
      ],
      "settings": {
        "foreground": "#dab997"
      }
    },
    {
      "name": "Punctuation - dimmed",
      "scope": [
        "punctuation",
        "meta.brace",
        "meta.bracket",
        "punctuation.separator",
        "punctuation.terminator"
      ],
      "settings": {
        "foreground": "#949494"
      }
    },
    {
      "name": "Invalid/Deprecated",
      "scope": [
        "invalid",
        "invalid.deprecated"
      ],
      "settings": {
        "foreground": "#d75f5f",
        "fontStyle": "strikethrough"
      }
    }
  ],
