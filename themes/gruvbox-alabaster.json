{
    "$schema": "vscode://schemas/color-theme",
    "name": "Gruvbox Alabaster",
    "type": "dark",
    "tokenColors": [
        {
            "name": "Base text - not highlighted",
            "scope": [
                "source",
                "text",
                "variable",
                "variable.other.readwrite",
                "variable.other.object",
                "variable.other.property",
                "variable.parameter",
                "support.variable",
                "meta.object-literal.key",
                "string.unquoted.plain.out.yaml",
                "entity.name.tag.yaml",
                "support.type.property-name.json",
                "support.type.property-name",
                "meta.structure.dictionary.key",
                "string.json support.type.property-name.json"
            ],
            "settings": {
                "foreground": "#d5c4a1"
            }
        },
        {
            "name": "Comments - brightest italic",
            "scope": ["comment", "punctuation.definition.comment"],
            "settings": {
                "foreground": "#949494",
                "fontStyle": "italic"
            }
        },
        {
            "name": "Strings",
            "scope": ["string", "string.quoted", "string.template", "string.regexp", "constant.other.symbol"],
            "settings": {
                "foreground": "#85ad85"
            }
        },
        {
            "name": "String escape sequences",
            "scope": ["constant.character.escape", "constant.other.placeholder"],
            "settings": {
                "foreground": "#949494"
            }
        },
        {
            "name": "Numbers and Constants",
            "scope": [
                "constant.numeric",
                "constant.character",
                "constant.language.boolean",
                "constant.language.null",
                "constant.language.undefined",
                "constant.language"
            ],
            "settings": {
                "foreground": "#afaf00"
            }
        },
        {
            "name": "Global definitions (functions, classes)",
            "scope": ["entity.name.function", "entity.name.class", "entity.name.type", "support.class", "support.type"],
            "settings": {
                "foreground": "#83adad"
            }
        },
        {
            "name": "Keywords - base text color",
            "scope": [
                "keyword",
                "keyword.control",
                "keyword.operator",
                "keyword.other",
                "storage.type",
                "storage.modifier"
            ],
            "settings": {
                "foreground": "#dab997"
            }
        },
        {
            "name": "Function/method calls - base text color",
            "scope": ["meta.function-call", "support.function"],
            "settings": {
                "foreground": "#dab997"
            }
        },
        {
            "name": "Punctuation - dimmed",
            "scope": ["punctuation", "meta.brace", "meta.bracket", "punctuation.separator", "punctuation.terminator"],
            "settings": {
                "foreground": "#949494"
            }
        },
        {
            "name": "Invalid/Deprecated",
            "scope": ["invalid", "invalid.deprecated", "invalid.illegal"],
            "settings": {
                "foreground": "#d75f5f",
                "fontStyle": "strikethrough"
            }
        }
    ],
    "colors": {
        // ========================================
        // Base UI Colors
        // ========================================
        "focusBorder": "#83adad",
        "descriptionForeground": "#949494",

        // ========================================
        // Editor
        // ========================================
        "editor.background": "#262626",
        "editor.foreground": "#dab997",
        "editor.lineHighlightBackground": "#3a3a3a",
        "editor.rangeHighlightBackground": "#4e4e4e2f",

        // Selection
        "editor.selectionBackground": "#4e4e4e80",
        "editor.selectionHighlightBackground": "#ffaf0044",
        "editor.selectionHighlightBorder": "#8a8a8a",

        // Find/Search
        "editor.findMatchBackground": "#ff870044",
        "editor.findMatchBorder": "#ffaf0055",
        "editor.findMatchHighlightBackground": "#ffaf0022",

        // Word Highlighting
        "editor.wordHighlightBackground": "#83adad2f",
        "editor.wordHighlightBorder": "#8a8a8a",
        "editor.wordHighlightStrongBackground": "#83adad40",
        "editor.wordHighlightStrongBorder": "#8a8a8a",

        // ========================================
        // Editor: Cursor & Line Numbers
        // ========================================
        "editorCursor.background": "#262626",
        "editorCursor.foreground": "#ebdbb2",
        "editorLineNumber.foreground": "#8a8a8a",
        "editorLineNumber.activeForeground": "#dab997",

        // ========================================
        // Editor: Brackets
        // ========================================
        "editorBracketHighlight.foreground1": "#83adad",
        "editorBracketHighlight.foreground2": "#85ad85",
        "editorBracketHighlight.foreground3": "#afaf00",
        "editorBracketMatch.background": "#4e4e4e2f",
        "editorBracketMatch.border": "#8a8a8a",

        // ========================================
        // Editor: Diagnostics (Errors, Warnings, Info)
        // ========================================
        "editorError.foreground": "#d75f5f",
        "editorWarning.foreground": "#ffaf00",
        "editorInfo.foreground": "#83adad",

        // ========================================
        // Editor: Gutter (Git indicators)
        // ========================================
        "editorGutter.addedBackground": "#85ad85",
        "editorGutter.deletedBackground": "#d75f5f",
        "editorGutter.modifiedBackground": "#83adad",

        // ========================================
        // Editor: Overview Ruler (Scrollbar indicators)
        // ========================================
        "editorOverviewRuler.background": "#262626",
        "editorOverviewRuler.border": "#3a3a3a",
        "editorOverviewRuler.addedBackground": "#85ad85",
        "editorOverviewRuler.deletedBackground": "#d75f5f",
        "editorOverviewRuler.modifiedBackground": "#83adad",
        "editorOverviewRuler.errorForeground": "#d75f5f",
        "editorOverviewRuler.warningForeground": "#ffaf00",
        "editorOverviewRuler.infoForeground": "#83adad",

        // ========================================
        // Editor: Widgets (Hover, Suggest, etc.)
        // ========================================
        "editorWidget.background": "#3a3a3a",
        "editorHoverWidget.background": "#3a3a3a",
        "editorHoverWidget.border": "#4e4e4e",
        "editorHoverWidget.highlightForeground": "#83adad",
        "editorSuggestWidget.background": "#3a3a3a",
        "editorSuggestWidget.border": "#4e4e4e",
        "editorSuggestWidget.selectedBackground": "#4e4e4e",

        // ========================================
        // Editor: Other
        // ========================================
        "editorWhitespace.foreground": "#4e4e4e",
        "editorIndentGuide.background1": "#4e4e4e",
        "editorIndentGuide.activeBackground1": "#8a8a8a",
        "editorRuler.foreground": "#4e4e4e",
        "editorInlayHint.background": "#3a3a3a",
        "editorInlayHint.foreground": "#949494",
        "editorMarkerNavigation.background": "#3a3a3a",

        // ========================================
